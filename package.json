{
  "name": "frontend-p3",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "uat": "vite build --mode uat",
    "preview": "vite preview",
    "test": "jest -c jest.config.js --colors --passWithNoTests",
    "coverage": "jest -c jest.config.js --colors --coverage --passWithNoTests",
    "lint:eslint": "eslint --ext .jsx,.js ./src && eslint --ext .jsx,.js",
    "lint:styles": "stylelint -- src/components/**/*.{js,jsx}",
    "lint": "npm run lint:eslint && npm run lint:styles && npm run prettier",
    "prettier": "prettier -c .prettierrc \"src/**/*.jsx\" --write",
    "prepare": "husky install",
    "pre-commit": "npm run prettier && npm run lint && npx --no-install lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-react": "^7.30.0",
    "framer-motion": "^6.5.1",
    "http-status-codes": "^2.2.0",
    "leaflet": "^1.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-input-range": "^1.3.0",
    "react-leaflet": "^4.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-switch": "^7.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@vitejs/plugin-react": "^1.3.2",
    "core-js": "^3.22.5",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-lodash": "^7.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "vite": "^2.9.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "git add ."
    ]
  }
}
